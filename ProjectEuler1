#include<stdio.h>
#include<stdlib.h>
#include<math.h>


int main()
{

	int number;

	printf(" Enter your number :");
	scanf_s(" %d", &number);

	int i;
	int sum = 0;
	for (i = 1; i < number; i++)
	{

		if (i % 3 == 0 || i % 5 == 0)
		{
		
			printf(" %d \n" ,i);
			sum = sum + i;
		}
	}

	printf("Total : %d", sum);
	return 0;
}
